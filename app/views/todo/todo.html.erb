<div ng-app="TaskManager">
  <div class="container">
    <div class="content" ng-controller="taskController">
      <h3>Your ToDo list</h3>
      <p class="tagline">Todo by Project-Awesome&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <a href=""><i class="fa fa-plus-square"></i></a>&nbsp;&nbsp;&nbsp;
      <a href=""><i class="fa fa-trash-o"></i></a>&nbsp;&nbsp;&nbsp;
      <a href=""><i class="fa fa-check"></i></a></p>

    <form action="" method="post">
      <div class="inputContainer">
          <input type="text" id="name"
                 class="taskName"
                 placeholder="Name"
                 ng-model="newTask">
      </div>
      <div class="inputContainer">
          <textarea
                  id="description"
                  class="taskDescription"
                  placeholder="Description"
                  ng-model="newTask"
                  rows="3"
                  cols="40"></textarea>
          <label for="description"></label>
      </div>
      <div class="inputContainer">
          <label>Date effective</label>
          <input type="date"
                 id="dateeffective"
                 class="taskDate"
                 ng-model="newTaskDate">
          <label>Date due</label>
          <input type="date"
                 id="duedate"
                 class="taskDate"
                 ng-model="newTaskDate">
      </div>

      <div class="inputContainer">
          <input type="text"
                 id="create_by"
                 class="createBy"
                 placeholder="Created by"
                 ng-model="create_by">
      </div>
      <hr/>
      <div class ="priority">
          <select>
            <option value="high Priority">High Priority</option>
            <option value ="low priority">Low Priority</option>
          </select>
      </div>
  </form>
